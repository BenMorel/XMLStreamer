{
  "name": "benmorel/xml-streamer",
  "description": "Stream large XML files as DOM nodes with low memory consumption",
  "type": "library",
  "keywords": [
    "XML",
    "XMLReader",
    "DOM"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-dom": "*",
    "ext-xmlreader": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "php-coveralls/php-coveralls": "^2.4",
    "ext-simplexml": "*",
    "vimeo/psalm": "6.13.1"
  },
  "autoload": {
    "psr-4": {
      "BenMorel\\XMLStreamer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenMorel\\XMLStreamer\\Tests\\": "tests/"
    }
  }
}
